<!DOCTYPE html>
<html lang="en">
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag    "application" %>
  <%= stylesheet_link_tag	"bootstrap" %>
  <%= stylesheet_link_tag	"activites" %>
  <%= stylesheet_link_tag	"courses" %>
  <%= stylesheet_link_tag	"events" %>
  <%= stylesheet_link_tag	"scaffolds" %>
  <%= stylesheet_link_tag	"static_pages" %>
  <%= javascript_include_tag "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
	<div id="fb-root"></div>
	<script>(function(d, s, id) {
	  var js, fjs = d.getElementsByTagName(s)[0];
	  if (d.getElementById(id)) return;
	  js = d.createElement(s); js.id = id;
	  js.src = "//connect.facebook.net/en_US/all.js#xfbml=1&appId=461745663914113";
	  fjs.parentNode.insertBefore(js, fjs);
	}(document, 'script', 'facebook-jssdk'));</script>
	<%= render 'layouts/header' %>
	<div class="container">
		<% flash.each do |type, message| %>
			<div class="alert <%=  flash_class type %>">
				<button class="close" data-dismiss="alert">&times;</button>
				<%= message %>
			</div>
		<% end %>
		
		<%= yield %>

	</div>
	<%= render 'layouts/overlay' %>
	<%= render 'layouts/ad' %>
	<%= render 'layouts/footer' %>
</body> 
</html>
