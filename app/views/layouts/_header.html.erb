<div class="navbar navbar-fixed-top">
  <div class="navbar-inner">
  	<div class="container">
    	<%= link_to image_tag("logo_small.jpg", :border=>0, :class=>'logo'), root_path, :class=>"brand" %>
	    <ul class="nav">
	    	<% if user_signed_in? %>
	      		<li><%= link_to "My Courses", courses_path %></li>
	      		<li><%= link_to "New Course", new_course_path %></li>
	      		<li><%= link_to "New Event", new_course_path %></li>
      		<% else %>
      			<li><%= link_to "About", about_path %></li>
      			<li><%= link_to "Help", help_path %></li>
      			<li><%= link_to "Contact", contact_path %></li>
  			<% end %>
	    </ul>
	    <ul class="nav pull-right">
	    	<% if user_signed_in? %>
				<li><%= link_to current_user.full_name, edit_user_registration_path %></li>
				<li><%= link_to "Log Out", logout_path, method: :delete %></li>
	    	<% else %>
	    		<li><%= link_to "Register", register_path %></li>
	    		<li><%= link_to "Login", login_path %></li>
	    		<li><%= link_to image_tag('fbsignin.png', :class=>'fb'), user_omniauth_authorize_path(:facebook) %></li>
	    	<% end %>
	    </ul>
    </div>
  </div>
