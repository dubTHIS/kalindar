<h1>Listing courses</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Body</th>
    <th>C name</th>
    <th>C desc</th>
    <th>C</th>
    <th>C school</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @courses.each do |course| %>
  <tr>
    <td><%= course.title %></td>
    <td><%= course.body %></td>
    <td><%= course.c_name %></td>
    <td><%= course.c_desc %></td>
    <td><%= course.c_id %></td>
    <td><%= course.c_school %></td>
    <td><%= link_to 'Show', course %></td>
    <td><%= link_to 'Edit', edit_course_path(course) %></td>
    <td><%= link_to 'Destroy', course, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

